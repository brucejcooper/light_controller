{
    "configurations": [
        {
            "name": "AVR",
            "includePath": [
                "${workspaceFolder}/**",
                "${workspaceFolder}/avr/include"
            ],
            "defines": [
                "__GNUC__=7",
                "__GNUC_MINOR__=3",
                "__ATtiny804__",
                "__AVR_ATtiny804__",
                "F_CPU=20000000"
            ],
            "compilerPath": "${workspaceFolder}/avr/bin/avr-gcc",
            "cStandard": "c11",
            "cppStandard": "c++17",
            "intelliSenseMode": "linux-gcc-x64",
        }
    ],
    "version": 4
}